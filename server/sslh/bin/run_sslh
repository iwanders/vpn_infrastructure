#!/bin/bash

VPNLAN='172.30.2'

LISTEN_IP=0.0.0.0
LISTEN_PORT=443
SSH_HOST=$VPNLAN.254
SSH_PORT=22
OPENVPN_HOST=$VPNLAN.3
OPENVPN_PORT=443
HTTPS_HOST=$VPNLAN.5
HTTPS_PORT=4000
TINC_HOST=$VPNLAN.1
TINC_PORT=655
sslh -f -u root -p $LISTEN_IP:$LISTEN_PORT --ssh $SSH_HOST:$SSH_PORT --ssl $HTTPS_HOST:$HTTPS_PORT --openvpn $OPENVPN_HOST:$OPENVPN_PORT --tinc $TINC_HOST:$TINC_PORT

